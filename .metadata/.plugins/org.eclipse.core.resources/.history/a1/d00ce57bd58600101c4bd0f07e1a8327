package day06;

public class Array06 {

	public static void main(String[] args) {
		// String 배열
		
		String[] arr = new String[5];
		arr[0] = "홍길동";
		arr[1] = "박영이";
		arr[2] = "최순자";
		arr[3] = "이길동";
		arr[4] = "황길범";
		
		for(String s:arr) {
			System.out.println(s);
		}
		
		int num = 11;
		int num2 = num;
		
		String[] arr2 = arr;
		for(String s:arr2) {
			System.out.println(s);
		}
	}

}
