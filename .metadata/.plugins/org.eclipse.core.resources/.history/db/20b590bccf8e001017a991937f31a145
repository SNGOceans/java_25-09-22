package day13;

import java.util.Date;
import java.util.Calendar;

public class Date01 {

	public static void main(String[] args) {
		// 날짜 시간 클래스
		/* Date 클래스 => deprecated(비권장)
		 * / Calendar / LocalDateTime
		 */
		
		Calendar rightNow = Calendar.getInstance();
		System.out.println(Calendar.DATE);
	}
	

}
