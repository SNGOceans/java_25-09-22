package day14;

import java.util.HashSet;
import java.util.Set;

public class SetEx01 {

    public static void main(String[] args) {
        Set<Integer> randomNum = new HashSet<>();

        // 중복 없는 랜덤 숫자 6개 생성
        while (randomNum.size() < 6) {
            int num = (int)(Math.random() * 45) + 1; // 1~45 사이
            randomNum.add(num); // 중복 자동 제거
        }

        // 출력
        System.out.println("--- 랜덤 숫자 6개 ---");
        for (int n : randomNum) {
            System.out.print(n + " ");
        }
    }
}