package day16;

import java.util.Arrays;

public class Stream03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = {1,3,2,6,4,3,2,6,8,8,5,3,5,4,4,3,2,5,5,6,4,3,6,7,6,4};
		
		// 스트림으로 구성하여 중복제거하고, 정렬하여, 짝수만 출력
				Arrays.stream(arr)
			  .distinct()
			  .sorted()
			  .filter(n -> n % 2 == 0)
			  .forEach(n -> System.out.print(n + " "));// 짝수만 필터링
				
		// 스트림으로 구성하여, 중복제거하고, 홀수만 배열로 리턴

	}

}
