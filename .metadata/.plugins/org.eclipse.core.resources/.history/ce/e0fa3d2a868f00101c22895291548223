package day14;

import java.util.ArrayList;

public class List03 {

	public static void main(String[] args) {
		/* 과일 5개 정도 저장하고 list 출력
		 * */
		
		ArrayList<String> list = new ArrayList<>();
		list.add("사과");
		list.add("바나나");
		list.add("오렌지");
		list.add("귤");
		list.add("배");
		list.add("감");
		list.add("참외");

		System.out.println(list);
	}

}
