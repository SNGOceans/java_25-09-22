import java.util.Arrays;
import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class BaseballGame {

    public static void main(String[] args) {
        
        // 1. ì»´í“¨í„°ì˜ 3ìë¦¬ ìˆ«ì ìƒì„± (ì¤‘ë³µ ì—†ì´)
        Set<Integer> comNumSet = new HashSet<>();
        while(comNumSet.size() < 3) {
            comNumSet.add((int)(Math.random() * 10));
        }
        Integer[] comNum = comNumSet.toArray(new Integer[0]);
        
        System.out.println("ì»´í“¨í„° ìˆ«ì(í…ŒìŠ¤íŠ¸ìš©): " + Arrays.toString(comNum));
        
        Scanner scan = new Scanner(System.in);
        int strike = 0;
        
        // 2. 3 ìŠ¤íŠ¸ë¼ì´í¬ê°€ ë  ë•Œê¹Œì§€ ê²Œì„ ë°˜ë³µ
        while(strike != 3) {
            strike = 0;
            int ball = 0;
            Integer[] userNum = new Integer[3];

            System.out.println("ìˆ«ì 3ê°œë¥¼ ë„ì–´ì“°ê¸°ë¡œ êµ¬ë¶„í•˜ì—¬ ì…ë ¥í•˜ì„¸ìš” (ì˜ˆ: 1 2 3) >");
            
            try {
                String input = scan.nextLine();
                String[] tokens = input.trim().split("\\s+");
                
                // ì…ë ¥ ê°œìˆ˜ í™•ì¸
                if (tokens.length != 3) {
                    System.out.println("âŒ ì˜ëª»ëœ ì…ë ¥ì…ë‹ˆë‹¤. ìˆ«ì 3ê°œë¥¼ ì…ë ¥í•´ì£¼ì„¸ìš”.");
                    continue; 
                }
                
                Set<Integer> userNumSet = new HashSet<>();
                boolean isValidInput = true;
                
                for (int i = 0; i < tokens.length; i++) {
                    int num = Integer.parseInt(tokens[i]);
                    
                    // ì…ë ¥ëœ ìˆ«ìì˜ ë²”ìœ„ (0-9) í™•ì¸
                    if (num < 0 || num > 9) {
                        System.out.println("âŒ ìˆ«ìëŠ” 0ë¶€í„° 9 ì‚¬ì´ì—¬ì•¼ í•©ë‹ˆë‹¤.");
                        isValidInput = false;
                        break;
                    }
                    
                    userNum[i] = num;
                    userNumSet.add(num);
                }
                
                if (!isValidInput) {
                    continue; 
                }
                
                // ì…ë ¥ëœ ìˆ«ìì˜ ì¤‘ë³µ í™•ì¸
                if (userNumSet.size() != 3) {
                    System.out.println("âŒ ì¤‘ë³µë˜ì§€ ì•ŠëŠ” ìˆ«ìë¥¼ ì…ë ¥í•´ì£¼ì„¸ìš”.");
                    continue;
                }
                
            } catch (NumberFormatException e) {
                System.out.println("âŒ ì˜ëª»ëœ ì…ë ¥ì…ë‹ˆë‹¤. ìˆ«ìë¥¼ ì…ë ¥í•´ì£¼ì„¸ìš”.");
                continue;
            }
            
            // 3. ìŠ¤íŠ¸ë¼ì´í¬ì™€ ë³¼ íŒì •
            for (int i = 0; i < userNum.length; i++) {
                for (int j = 0; j < comNum.length; j++) {
                    if (userNum[i].equals(comNum[j])) {
                        if (i == j) {
                            strike++;
                        } else {
                            ball++;
                        }
                        break;
                    }
                }
            }
            
            // 4. ê²°ê³¼ ì¶œë ¥
            if (strike == 0 && ball == 0) {
                System.out.println("OUT!");
            } else {
                System.out.println(strike + "S " + ball + "B");
            }
        }
        
        System.out.println("ğŸ‰ ì •ë‹µì…ë‹ˆë‹¤! ê²Œì„ì„ ì¢…ë£Œí•©ë‹ˆë‹¤.");
        scan.close();
    }
}